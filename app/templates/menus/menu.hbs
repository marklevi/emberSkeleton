<div class="menu">
    <div class="location__menu-title">{{model.menu.displayTitle}}</div>
    {{#each model.menu.submenus as |submenu|}}
        <div class="location__submenu-title">{{submenu.displayTitle}}</div>
        <ul>
            {{#each submenu.menuItems as |menuItem|}}
                <li>
                  <span class="menu-item">{{menuItem.displayTitle}}</span>
                  <span class="price-item">{{price-resolver menuItem model.priceBand}}</span>
                </li>
            {{/each}}
        </ul>
    {{/each}}
</div>
